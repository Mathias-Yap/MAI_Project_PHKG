PREFIX sphn: <https://www.biomedit.ch/rdf/sphn-schema/sphn/>
                SELECT ?code WHERE {
                <https://www.biomedit.ch/rdf/sphn-schema/sphn/LabTestEvent/6/patients/12412870> sphn:hasSubjectPseudoIdentifier <https://www.biomedit.ch/rdf/sphn-schema/sphn/PATIENTS/12412870> ;
                                    sphn:hasLabTest ?labResult .
                ?labResult sphn:hasCode ?code .
                }
