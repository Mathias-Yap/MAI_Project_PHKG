PREFIX sphn: <https://www.biomedit.ch/rdf/sphn-schema/sphn/>
                SELECT ?code WHERE {
                <https://www.biomedit.ch/rdf/sphn-schema/sphn/LabTestEvent/15/patients/19807255> sphn:hasSubjectPseudoIdentifier <https://www.biomedit.ch/rdf/sphn-schema/sphn/PATIENTS/19807255> ;
                                    sphn:hasLabTest ?labResult .
                ?labResult sphn:hasCode ?code .
                }
