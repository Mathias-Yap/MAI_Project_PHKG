                <https://www.biomedit.ch/rdf/sphn-schema/sphn/LabTestEvent/1/patients/15215857> sphn:hasSubjectPseudoIdentifier <https://www.biomedit.ch/rdf/sphn-schema/sphn/PATIENTS/15215857> ;
                                    sphn:hasLabTest ?labResult .
                ?labResult sphn:hasCode ?code .
                }
