PREFIX sphn: <https://www.biomedit.ch/rdf/sphn-schema/sphn/>
                SELECT ?code WHERE {
                <https://www.biomedit.ch/rdf/sphn-schema/sphn/LabTestEvent/1/patients/10313626> sphn:hasSubjectPseudoIdentifier <https://www.biomedit.ch/rdf/sphn-schema/sphn/PATIENTS/10313626> ;
                                    sphn:hasLabTest ?labResult .
                ?labResult sphn:hasCode ?code .
                }
