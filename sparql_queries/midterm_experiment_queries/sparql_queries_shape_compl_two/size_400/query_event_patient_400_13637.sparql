PREFIX sphn: <https://www.biomedit.ch/rdf/sphn-schema/sphn/>
                SELECT ?code WHERE {
                <https://www.biomedit.ch/rdf/sphn-schema/sphn/LabTestEvent/8/patients/15404950> sphn:hasSubjectPseudoIdentifier <https://www.biomedit.ch/rdf/sphn-schema/sphn/PATIENTS/15404950> ;
                                    sphn:hasLabTest ?labResult .
                ?labResult sphn:hasCode ?code .
                }
