                <https://www.biomedit.ch/rdf/sphn-schema/sphn/LabTestEvent/51/patients/15371742> sphn:hasSubjectPseudoIdentifier <https://www.biomedit.ch/rdf/sphn-schema/sphn/PATIENTS/15371742> ;
                                    sphn:hasLabTest ?labResult .
                ?labResult sphn:hasCode ?code .
                }
